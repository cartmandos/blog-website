---
import PageView from "@/components/Page/PageView.astro"
import Terminal from "@/components/Terminal/Terminal.astro"
import MainLayout from "@/layouts/MainLayout.astro"

const initTerminalOutput = [
  {
    command: "echo 'Welcome to my portfolio!'",
    output: [
      "Welcome to my interactive terminal portfolio.",
      "Type 'help' to see available commands.",
      "",
    ],
  },
]
---

<MainLayout title="About">
  <PageView type="markdown" id="about" title="About" />
  <Terminal initialOutput={initTerminalOutput} />
</MainLayout>
