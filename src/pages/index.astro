---
import Terminal from "@/components/Terminal/Terminal.astro"
import MainLayout from "@/layouts/MainLayout.astro"
import { INIT_OUTPUT} from "@/constants"


const initTerminalOutput = [
  {
    command: "site init -y && cat welcome",
    output: [
    INIT_OUTPUT.welcome,
      INIT_OUTPUT.help,
    ],
  },
]
---

<MainLayout title="Terminal Portfolio">
    <Terminal initialOutput={initTerminalOutput} />
</MainLayout>

